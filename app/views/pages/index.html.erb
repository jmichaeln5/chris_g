              <div class="input-group input-group-newsletter">

                <div class="input-group-append px-2">
                  <button class="btn btn-secondary " type="button" id="submit-button">
                    <%= link_to "Login", new_user_session_path, style:"color: white !important;" unless current_user  %>
                    <% if user_signed_in? %>
                      <%= link_to "Logout", destroy_user_session_path, method: :delete, style:"color: white !important;" %>
                    <% end %>
                  </button>
                </div>


                <div class="input-group-append">
                  <button class="btn btn-secondary " type="button" id="submit-button">
                    <%= link_to "Register", new_user_registration_path, style:"color: white !important;" unless current_user  %>
                    <%= link_to "Account Settings", edit_user_registration_path, style:"color: white !important;" if user_signed_in? %>                  </button>
                </div>


              </div>


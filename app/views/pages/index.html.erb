.overlay {
position: absolute;
top: 0;
left: 0;
height: 100%;
width: 100%;
background-color: #cd9557;
opacity: 0.7;
z-index: 1;
}

.masthead {
position: relative;
overflow: hidden;
padding-bottom: 3rem;
z-index: 2;
}

.masthead .masthead-bg {
position: absolute;
top: 0;
bottom: 0;
right: 0;
left: 0;
width: 100%;
min-height: 35rem;
height: 100%;
background-color: rgba(0, 46, 102, 0.8);
transform: skewY(4deg);
transform-origin: bottom right;
}

.masthead .masthead-content h1 {
font-size: 2.5rem;
}

.masthead .masthead-content p {
font-size: 1.2rem;
}

.masthead .masthead-content p strong {
font-weight: 700;
}

